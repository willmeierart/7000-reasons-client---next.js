{
  "name": "7000-reasons",
  "version": "1.0.0",
  "description": "artist experiment",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development node server/index.js",
    "prebuild": "rimraf \"dist/functions\"",
    "build": "npm run build-functions && npm run build-app",
    "postbuild": "cd \"dist/functions\" && npm i",
    "build-app": "cd \"src/app\" && npm run build",
    "build-functions": "babel \"src/functions\" --out-dir \"dist/functions\" --copy-files --ignore \"node_modules\"",
    "predeploy": "npm run build",
    "deploy": "firebase deploy",
    "format": "prettier --write \"**/**{.js, .json}\"",
    "postinstall": "npm run install-app && npm run install-functions",
    "install-app": "cd \"src/app\" && npm i",
    "install-functions": "cd \"src/functions\" && npm i",
    "next": "cd \"src/app\" && npm run dev",
    "preserve": "npm run build",
    "serve": "firebase serve",
    "start": "NODE_ENV=production node server/index.js",
    "export": "next build && next export"
  },
  "author": "Will Meier",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.3",
    "autoprefixer": "^7.1.6",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "graphql": "^0.11.7",
    "graphql-anywhere": "^4.0.1",
    "graphql-tag": "^2.5.0",
    "isomorphic-fetch": "^2.2.1",
    "loaders.css": "^0.1.2",
    "next": "^4.1.4",
    "next-routes": "^1.1.0",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.9",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.1.1",
    "react-loaders": "^3.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "prop-types": "^15.6.0",
    "standard": "^10.0.3"
  }
}
